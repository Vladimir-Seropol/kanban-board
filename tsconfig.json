{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "esnext"],
      "jsx": "react",
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,  
      "outDir": "./dist"
    },
    "include": ["src"]
  }
  